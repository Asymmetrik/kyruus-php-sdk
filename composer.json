{
  "name": "asymmetrik/kyruus-sdk-php",
  "description": "Kyruus API wrapping in PHP",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Asymmetrik",
      "email": "apollo@asymmetrik.com"
    },
    {
      "name": "Christopher Sidell",
      "email": "csidell@asymmetrik.com"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^6.2",
    "league/oauth2-client": "^1.4",
    "spatie/regex": "^1.1",
    "doctrine/collections": "^1.4"
  },
  "keywords": [
    "kyruus",
    "api",
    "sdk"
  ],
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
